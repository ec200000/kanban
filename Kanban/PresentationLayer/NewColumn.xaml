<Window x:Class="Kanban.PresentationLayer.ViewModel.NewColumn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kanban.PresentationLayer.ViewModel"
        mc:Ignorable="d"
        Title="NewColumn" Height="250" Width="400">
    <Grid Background="#FFFFFB81">
        <TextBox Name ="column" Text="{Binding Column}" HorizontalAlignment="Left" Height="37" Margin="31,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186"/>
        <Button Content="CREATE" HorizontalAlignment="Left" Margin="142,139,0,0" Background="#FFFEFF9F" VerticalAlignment="Top" Width="75" Click="Button_Click" FontSize="20"/>
        <Button Content="RETURN" VerticalAlignment="Top" Height="30" Width="110" Background="{x:Null}" Click="Button_Click_1" FontSize="20" BorderBrush="{x:Null}" Margin="0,0,283.6,0"/>

    </Grid>
</Window>
