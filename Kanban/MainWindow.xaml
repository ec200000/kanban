<Window x:Class="Kanban.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kanban"
        mc:Ignorable="d"
        Title="SignUp" Height="300" Width="300">
    <Grid Background="#FF8EC3DA">
        <Label Content="Email:" HorizontalAlignment="Left" Margin="50,66,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="50,114,0,0" VerticalAlignment="Top"/>
        <TextBox Name="emal" Text="{Binding Email}" HorizontalAlignment="Left" Height="23" Margin="132,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="pswrd" Text="{Binding PWD}" HorizontalAlignment="Left" Height="23" Margin="132,122,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="SignUp" HorizontalAlignment="Left" Margin="111,200,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Background="#FFE4D369" Foreground="Black"/>
        <Button Content="already signed up? login here" HorizontalAlignment="Left" Margin="70,230,0,0" VerticalAlignment="Top" Click="Button_Click2" BorderBrush="{x:Null}" Foreground="Black" Background="{x:Null}"/>
    </Grid>
</Window>
